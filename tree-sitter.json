{
  "grammars": [
    {
      "name": "pact",
      "camelcase": "Pact",
      "scope": "source.pact",
      "path": ".",
      "file-types": ["pact", "repl"],
      "highlights": ["queries/highlights.scm"],
      "locals": ["queries/locals.scm"],
      "tags": ["queries/tags.scm"],
      "injection-regex": "^(pact|repl)$"
    }
  ],
  "metadata": {
    "version": "0.0.9",
    "license": "MIT",
    "description": "Pact grammar for tree-sitter",
    "authors": [
      {
        "name": "Salama Ashoush",
        "email": "salamaashoush@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-pact"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "java": true,
    "kotlin": true
  }
}
