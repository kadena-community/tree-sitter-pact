{
  "name": "tree-sitter-pact",
  "version": "1.0.0",
  "description": "Pact grammar for tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "build:wasm": "tree-sitter build-wasm",
    "playground": "tree-sitter build-wasm && tree-sitter playground playground.pact",
    "parse:test": "tree-sitter parse test.pact",
    "parse:coin": "tree-sitter parse coin.pact"
  },
  "keywords": [
    "tree-sitter",
    "pact",
    "kadena"
  ],
  "author": "Salama Ashoush <salamaashoush@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.pact",
      "file-types": [
        "pact",
        "repl"
      ],
      "highlights": [
        "queries/highlights.scm",
        "queries/locals.scm"
      ],
      "injection-regex": "^(pact|repl)$"
    }
  ]
}
