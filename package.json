{
  "name": "tree-sitter-pact",
  "version": "0.0.5",
  "description": "Pact grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "lint": "eslint grammar.js",
    "test": "tree-sitter test && node --test bindings/node/*_test.js",
    "prepublish": "tree-sitter build",
    "parse:test": "tree-sitter parse test.pact",
    "parse:coin": "tree-sitter parse coin.pact"
  },
  "keywords": [
    "tree-sitter",
    "pact",
    "kadena"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "Salama Ashoush <salamaashoush@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.1.0",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "eslint": "^9.11.0",
    "globals": "^15.9.0",
    "node-gyp": "^10.2.0",
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-cli": "^0.23.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.pact",
      "file-types": [
        "pact",
        "repl"
      ],
      "tags": [
        "queries/tags.scm"
      ],
      "folds": [
        "queries/folds.scm"
      ],
      "indents": [
        "queries/indents.scm"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ],
      "injection-regex": "^(pact|repl)$"
    }
  ]
}
